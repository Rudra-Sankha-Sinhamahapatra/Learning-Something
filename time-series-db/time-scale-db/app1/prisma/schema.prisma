generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id    String @id @default(cuid())
  email String @unique
  name  String
}

model WebsiteUptime {
  time     Unsupported("timestamptz")
  website  String
  status   String
  
  @@ignore
  @@map("website_uptime") 
}